<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_height="wrap_content"
    android:layout_width="match_parent">

    <RelativeLayout
        android:id="@+id/Jagran"
        android:paddingTop="10dp"
        android:paddingRight="15dp"
        android:paddingLeft="15dp"
        android:paddingBottom="5dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <ImageView
            android:id="@+id/logo9"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:src="@drawable/ic_menu_send"
            android:contentDescription="image1"
            android:scaleType="fitXY"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@android:color/black"
            android:layout_toRightOf="@id/logo9"
            android:layout_centerVertical="true"
            android:textSize="20sp"
            android:layout_marginLeft="30dp"
            android:fontFamily="@font/neuton"
            android:text="Jagraan"/>

        <Switch
            android:id="@+id/SwitchJagraan"
            android:layout_width="55dp"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/Bhaskar"
        android:paddingTop="10dp"
        android:paddingRight="15dp"
        android:paddingLeft="15dp"
        android:paddingBottom="5dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <ImageView
            android:id="@+id/logo12"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:src="@drawable/ic_menu_send"
            android:contentDescription="image1"
            android:scaleType="fitXY"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@android:color/black"
            android:layout_toRightOf="@id/logo12"
            android:layout_centerVertical="true"
            android:textSize="20sp"
            android:layout_marginLeft="30dp"
            android:fontFamily="@font/neuton"
            android:text="Bhaskar"/>

        <Switch
            android:id="@+id/SwitchBhaskar"
            android:layout_width="55dp"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            />
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/LiveHindustan"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="5dp"
        android:paddingLeft="15dp"
        android:paddingRight="15dp"
        android:paddingTop="10dp">

        <ImageView
            android:id="@+id/logo10"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:contentDescription="image1"
            android:scaleType="fitXY"
            android:src="@drawable/ic_menu_send" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="30dp"
            android:layout_toRightOf="@id/logo10"
            android:fontFamily="@font/neuton"
            android:text="LiveHindustan"
            android:textColor="@android:color/black"
            android:textSize="20sp" />

        <Switch
            android:id="@+id/SwitchLiveHindustan"
            android:layout_width="55dp"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true" />
    </RelativeLayout>
</LinearLayout>